{
  "name": "@workspace/models-types",
  "version": "1.0.0",
  "description": "Type definitions and schemas for frontend use",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "default": "./dist/schemas/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "echo 'Built by @workspace/models'",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
