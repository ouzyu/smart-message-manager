{
  "name": "@workspace/slack",
  "version": "1.0.0",
  "description": "Slack Bolt and Slack Web API wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./clients": {
      "types": "./dist/clients/index.d.ts",
      "default": "./dist/clients/index.js"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "default": "./dist/services/index.js"
    },
    "./builders": {
      "types": "./dist/builders/index.d.ts",
      "default": "./dist/builders/index.js"
    },
    "./controllers": {
      "types": "./dist/controllers/index.d.ts",
      "default": "./dist/controllers/index.js"
    },
    "./routes": {
      "types": "./dist/routes/index.d.ts",
      "default": "./dist/routes/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@slack/bolt": "^4.4.0",
    "@slack/web-api": "^7.9.3",
    "@slack/types": "^2.15.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^22.0.0"
  },
  "peerDependencies": {
    "@workspace/models-client": "*"
  }
}
